<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bd.mapper.QuestionBankMapper">

    <resultMap id="BaseResultMap" type="com.bd.model.QuestionBank">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="bankType" column="bank_type" jdbcType="INTEGER"/>
            <result property="iconUrl" column="icon_url" jdbcType="VARCHAR"/>
            <result property="bankDesc" column="bank_desc" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="isDeleted" column="is_deleted" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,bank_type,icon_url,
        bank_desc,create_time,update_time,
        is_deleted
    </sql>
</mapper>
